{
	"enableAutoDiscovery": true,
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es2018",
		"sourceMap": true,
		"module": "ES2020",
		"moduleResolution": "node",
		"newLine": "LF",
		"removeComments": true,
		"noImplicitThis": true,
		"suppressImplicitAnyIndexErrors": true,
		"esModuleInterop" : true
	},
	"skipLibCheck": true
}
