{
	"enableAutoDiscovery": true,
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es2018",
//		"module": "amd",
//		"outFile": "main.js",
		"module": "ES2015",
		"moduleResolution": "node",
		"newLine": "LF",
		"removeComments": true,
		"noImplicitThis": true,
		"suppressImplicitAnyIndexErrors": true,
		"esModuleInterop" : true,
	},
	"skipLibCheck": true
}
